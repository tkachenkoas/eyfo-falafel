{
    "extends": "../tslint.json",
    "rules": {
        "no-trailing-whitespace": [true, "ignore-blank-lines"],
        "no-debugger" : false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
